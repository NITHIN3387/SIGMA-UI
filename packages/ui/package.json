{
  "name": "@sigma/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/styles/globals.css",
    ".": "./src/components/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles/globals.css",
    "lint": "eslint src/",
    "dev": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles/globals.css --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@sigma/eslint-config": "workspace:*",
    "@sigma/tailwind-config": "workspace:*",
    "@sigma/typescript-config": "workspace:*",
    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "next": "^14.2.3",
    "tailwind-merge": "^2.6.0"
  }
}
